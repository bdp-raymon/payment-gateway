{
    "name": "alish/payment-gateway",
    "description": "A laravel package for managing payment gateway",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ali Shabani",
            "email": "pryamidalish@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^7.1"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Alish\\PaymentGateway\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alish\\PaymentGateway\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alish\\PaymentGateway\\PaymentGatewayProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
